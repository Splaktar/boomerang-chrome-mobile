<?xml version='1.0' encoding='utf-8'?>
<widget id="org.gdgspacecoast" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GDG Space Coast</name>
    <description>GDG Space Coast Chrome mobile app.</description>
    <author />
    <preference name="KeyboardShrinksView" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <content src="plugins/org.chromium.bootstrap/chromeapp.html" />
    <access origin="https://apis.google.com/*" />
    <access origin="https://www.googleapis.com/*" />
    <access origin="https://picasaweb.google.com/*" />
    <access origin="*" />
</widget>
